<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_trip_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="trip">
                <div class="page">
                    <h2>Trip Report</h2>
                    <p>Trip Name: <t t-esc="trip.name"/></p>
                </div>
            </t>
        </t>
    </template>
</odoo>
